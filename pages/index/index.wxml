<view>
  <view class="search-bar">
    <view class="left-btn">
      <image src="../../icons/icon-arrow-left.png"></image>
    </view>
    <view class="search-input">
      <input placeholder="请输入" placeholder-style="color:#fff;"/>
    </view>
    <view class="right-btn">
      <image src="../../icons/icon-shopcar.png"></image>
    </view>
  </view>
  <swiper class="banner-slide"
          indicator-dots="{{bannerSlider.indicatorDots}}"
          indicator-color="rgba(0,0,0,0.5)"
          indicator-active-color="#ff4400"
          circular="{{bannerSlider.circular}}"
          autoplay="{{bannerSlider.autoplay}}"
          interval="{{bannerSlider.interval}}"
          duration="{{bannerSlider.duration}}">
    <block wx:for="{{bannerSlider.imgUrls}}" wx:key="unique">
      <swiper-item>
        <navigator url="/pages/good/index">
          <image src="{{item}}"/>
        </navigator>
      </swiper-item>
    </block>
  </swiper>
  <view class="category-list clear">
    <view class="category-item">
      <navigator class="link" url="/pages/good/index">
        <image src="../../icons/index-category1.png"></image>
        <text class="text">主材类</text>
      </navigator>
    </view>
    <view class="category-item">
      <navigator class="link" url="/pages/good/index">
        <image src="../../icons/index-category2.png"></image>
        <text class="text">辅料类</text>
      </navigator>
    </view>
