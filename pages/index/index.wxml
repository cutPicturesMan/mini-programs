    </view>
    <view class="category-item">
      <navigator class="link" url="/pages/good/index">
        <image src="../../icons/index-category5.png"></image>
        <text class="text">外购成品</text>
      </navigator>
    </view>
  </view>
  <swiper class="text-slider"
          circular="{{bannerSlider.circular}}"
          autoplay="{{textSlider.autoplay}}"
          interval="{{textSlider.interval}}"
          duration="{{textSlider.duration}}"
          vertical="{{textSlider.vertical}}">
    <block wx:for="{{textSlider.imgUrls}}" wx:key="unique">
      <swiper-item>
        <image src="../../icons/icon-trumpet.png" class="slide-image"/>
        <text>{{item}}</text>
      </swiper-item>
    </block>
  </swiper>
